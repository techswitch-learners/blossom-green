{
  "root": true,
  "env": { "browser": true, "es6": true },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "globals": {
    "process": true
  },
  "ignorePatterns": ["build"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "react/prop-types": "off"
  }
}
